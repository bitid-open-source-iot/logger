{
    "production": false,
    "authentication": false,
    "auth": {
        "host": "xxx",
        "token": "xxx",
        "appId": "xxx",
        "email": "xxx@xxx.co.za"
    },
    "logger": {
        "appenders": {
            "logger": {
                "type": "file",
                "backups": 10,
                "filename": "./logs/logger.log",
                "absolute": true,
                "maxLogSize": 102400
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "logger"
                ],
                "level": "error"
            }
        }
    },
    "mongodb": {
        "url": "xxx",
        "database": "xxx"
    },
    "websocket": {
        "port": 9505
    },
    "localwebserver": {
        "port": 9500
    }
}